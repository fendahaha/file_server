<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC
        "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>

    <!-- 数据库连接的信息：驱动类、连接地址、用户名、密码 -->
    <context id="MySqlContext" targetRuntime="MyBatis3">
        <commentGenerator>
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
        <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/file_server"
                        userId="file_server"
                        password="file_server">
        </jdbcConnection>

        <!-- 默认生成的模型类的位置 -->
        <javaModelGenerator targetPackage="com.example.file_server.entity" targetProject="src/main/java"/>

        <!-- 默认生成的映射文件的位置 -->
        <sqlMapGenerator targetPackage="mybatis.mapper" targetProject="src/main/resources"/>

        <!-- 默认生成的Mapper接口的位置 -->
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.example.file_server.mapper"
                             targetProject="src/main/java"/>

        <!-- 指定数据库表 -->
        <!--        <table tableName="upload_file" domainObjectName="UploadFile"/>-->
        <!--        <table tableName="user" domainObjectName="User"/>-->
<!--                <table tableName="room" domainObjectName="Room"/>-->
        <!--        <table tableName="config" domainObjectName="Config"/>-->
<!--        <table tableName="anchor" domainObjectName="Anchor"/>-->
<!--        <table tableName="gift" domainObjectName="Gift"/>-->
<!--        <table tableName="client" domainObjectName="Client"/>-->
        <table tableName="gift_send_record" domainObjectName="GiftSendRecord"/>
<!--        <table tableName="srs_streams" domainObjectName="SrsStreams">-->
<!--            <property name="useActualColumnNames" value="true"/>-->
<!--        </table>-->
<!--        <table tableName="srs_sessions" domainObjectName="SrsSessions">-->
<!--            <property name="useActualColumnNames" value="true"/>-->
<!--        </table>-->
<!--        <table tableName="srs_hls" domainObjectName="SrsHls">-->
<!--            <property name="useActualColumnNames" value="true"/>-->
<!--        </table>-->
    </context>

</generatorConfiguration>
